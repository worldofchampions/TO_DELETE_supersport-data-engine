<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings configSource="secrets.config" xdt:Transform="SetAttributes(configSource)">
    <add xdt:Transform="RemoveAll" />
  </connectionStrings>
  <appSettings>
    <add key="MongoDbName" value="SuperSportDataEngine_RawPayloadData" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets>
      <target xsi:type="WebService"
         name="MSTeams"
         url="https://outlook.office.com/webhook/37cf758b-461f-4119-8827-029240d8d40f@5313c061-2dd7-4e7c-aa59-b31ab4ca92a2/IncomingWebhook/7f7a74f84ec34426a21bf4bccdba86a7/f167c590-a64a-4f82-aeb1-d3d95ed3dc08"
         protocol="JsonPost"
         encoding="UTF-8"
         includeBOM="false"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)">
        <parameter name="text" type="System.String" layout="${longdate} ${newline} ${level} ${newline} ${message}"/>
      </target>
    </targets>
  </nlog>
</configuration>