  <appSettings>
    <add key="HangfireDashboardUrl" value="http://localhost:9622" />
    <!-- Jobs that are completed in hangfire will be deleted in the following number of days. -->
    <add key="JobExpirationTimeoutInDays" value="20" />
    <!-- Settings for reference data -->
    <add key="FixedScheduledJob_ReferenceData_JobId" value="Rugby→StatsProzone→FixedScheduleJob→ReferenceData" />
    <!-- Everyday at 2:00am -->
    <add key="FixedScheduledJob_ReferenceData_JobCronExpression" value="0 2 * * *" />
    <!-- Settings for Player Stats -->
    <add key="ScheduleManagerJob_PlayerStats_CurrentTournaments_JobIdPrefix" value="Rugby→StatsProzone→ScheduleManagerJob→PlayerStats→" />
    <add key="ScheduleManagerJob_PlayerStats_CurrentTournaments_JobCronExpression" value="0 */12 * * *" />
    <!-- Settings for fixtures -->
    <add key="FixedScheduledJob_FixturesData_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Fixtures" />
    <!-- Everyday at 2:05am -->
    <add key="FixedScheduledJob_FixturesData_JobCronExpression" value="0 2 * * *" />
    <add key="FixedScheduledJob_Lineups_Hourly_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Lineups" />
    <add key="FixedScheduledJob_Lineups_Hourly_jobCronExpression" value="0 * * * *" />
    <add key="FixedScheduledJob_Lineups_PastGames_Hourly_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Lineups→PastFixtures" />
    <add key="FixedScheduledJob_Lineups_PastGames_Hourly_jobCronExpression" value="0 * * * *" />
    <add key="FixedScheduledJob_Cleanup_Monthly_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Cleanup→TrackingTables" />
    <add key="FixedScheduledJob_Cleanup_Monthly_JobCronExpression" value="0 0 1 * *" />
    <!-- Settings for logs -->
    <add key="FixedScheduledJob_LogsData_ActiveTournaments_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Logs→ActiveTournaments" />
    <!-- Everyday at 2:05am -->
    <add key="FixedScheduledJob_LogsData_ActiveTournaments_JobCronExpression" value="0 2 * * *" />
    <!-- Settings for logs -->
    <add key="FixedScheduledJob_LogsData_CurrentTournaments_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Logs→CurrentTournaments" />
    <!-- Every 1 hour -->
    <add key="FixedScheduledJob_LogsData_CurrentTournaments_JobCronExpression" value="*/15 * * * *" />
    <!-- Settings for fixtures -->
    <add key="ScheduleManagerJob_Fixtures_CurrentTournaments_JobIdPrefix" value="Rugby→StatsProzone→ScheduleManagerJob→Fixtures→" />
    <add key="ScheduleManagerJob_Fixtures_CurrentTournaments_JobCronExpression" value="*/2 * * * *" />
    <!-- Settings for fixtures -->
    <add key="ScheduleManagerJob_Fixtures_ActiveTournaments_JobIdPrefix" value="Rugby→StatsProzone→ScheduleManagerJob→ChildJob→Fixtures→OneMonthFixtures→" />
    <add key="ScheduleManagerJob_Fixtures_ActiveTournaments_JobCronExpression" value="0 * * * *" />
    <!-- Settings for logs manager-->
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobIdPrefix" value="Rugby→StatsProzone→ScheduleManagerJob→Logs→" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobCronExpression_15Minutes" value="*/15 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobCronExpression_OneMinute" value="*/1 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_HighFrequencyPolling_CronExpression" value="*/1 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_HighFrequencyPolling_ExpiryInMinutes" value="15" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_LowFrequencyPolling_CronExpression" value="*/15 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_LowFrequencyPolling_ExpiryInMinutes" value="60" />
    <!-- Settings for results -->
    <!-- Everyday at 2:05am for a week's data-->
    <add key="FixedScheduledJob_ResultsData_Daily_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Results→AllFixtures" />
    <add key="FixedScheduledJob_ResultsData_Daily_PollingExpression" value="5 2 * * *" />
    <!-- Every hour for the next 3 days-->
    <add key="FixedScheduledJob_ResultsData_Hourly_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Results→EndedFixtures" />
    <add key="FixedScheduledJob_ResultsData_Hourly_PollingExpression" value="0 * * * *" />
    <!-- Every 15 minutes on fixture day-->
    <add key="FixedScheduledJob_ResultsData_OnFixtureDay_JobId" value="Rugby→StatsProzone→FixedScheduleJob→Results→CurrentDayFixtures" />
    <add key="FixedScheduledJob_ResultsData_OnFixtureDay_PollingExpression" value="*/15 * * * *" />
    <!-- End Settings for results -->
    <add key="LiveManagerJob_LiveMatch_JobIdPrefix" value="Rugby→StatsProzone→LiveManagerJob→LiveMatch→" />
    <add key="LiveManagerJob_LiveMatch_JobCronExpression" value="0 0 29 2/12000 WED" />
    <add key="FixedScheduledJob_LiveData_PastFixtures_JobIdPrefix" value="Rugby→StatsProzone→FixedManagerJob→IngestPastDataForFixtures" />
    <add key="FixedScheduledJob_LiveData_PastFixtures_JobCronExpression" value="0 2 * * *" />
    <add key="FixedScheduledJob_LiveData_PastFewDaysFixtures_JobIdPrefix" value="Rugby→StatsProzone→FixedManagerJob→IngestPastFewDaysDataForFixtures" />
    <add key="FixedScheduledJob_LiveData_PastFewDaysFixtures_JobCronExpression" value="0 2 * * *" />
    <add key="FixedScheduledJob_PastSeasonsFixtures_ActiveTournaments_JobIdPrefix" value="Rugby→StatsProzone→FixedManagerJob→IngestPastSeasonsForActiveTournaments" />
    <add key="FixedScheduledJob_PastSeasonsFixtures_ActiveTournaments_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Settings for motor drivers manager-->
    <!--<add key="ScheduleManagerJob_Drivers_CurrentLeagues_JobIdPrefix" value="Motor→StatsProzone→ScheduleManagerJob→Drivers→" />
    <add key="ScheduleManagerJob_Drivers_CurrentLeagues_JobCronExpression_15Minutes" value="*/15 3-23,0-1 * * *" />
    <add key="ScheduleManagerJob_Drivers_CurrentLeagues_JobCronExpression_OneMinute" value="*/1 3-23,0-1 * * *" />-->
    <!-- Settings for motorsport reference data-->
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_Leagues_JobId" value="Motorsport→Stats→FixedScheduleJob→Leagues" />
    <add key="MotorsportFixedScheduleJob_Leagues_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_Seasons_JobId" value="Motorsport→Stats→FixedScheduleJob→Seasons" />
    <add key="MotorsportFixedScheduleJob_Seasons_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_Races_JobId" value="Motorsport→Stats→FixedScheduleJob→Races" />
    <add key="MotorsportFixedScheduleJob_Races_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_RaceEvents_JobId" value="Motorsport→Stats→FixedScheduleJob→RaceEvents" />
    <add key="MotorsportFixedScheduleJob_RaceEvents_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_Drivers_JobId" value="Motorsport→Stats→FixedScheduleJob→Drivers" />
    <add key="MotorsportFixedScheduleJob_Drivers_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_Teams_JobId" value="Motorsport→Stats→FixedScheduleJob→Teams" />
    <add key="MotorsportFixedScheduleJob_Teams_JobCronExpression" value="30 2 * * *" />
    <!-- END: Settings for motorsport reference data-->
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_ActiveLeaguesRaceEventsGrids_JobId" value="Motorsport→Stats→FixedScheduleJob→RaceEventsGrids" />
    <add key="MotorsportFixedScheduleJob_ActiveLeaguesRaceEventsGrids_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_RaceEventsResults_JobId" value="Motorsport→Stats→FixedScheduleJob→RaceEventsResults" />
    <add key="MotorsportFixedScheduleJob_RaceEventsResults_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_DriverStandings_JobId" value="Motorsport→Stats→FixedScheduleJob→DriverStandings" />
    <add key="MotorsportFixedScheduleJob_DriverStandings_JobCronExpression" value="30 2 * * *" />
    <!-- Runs everyday at 2:30am -->
    <add key="MotorsportFixedScheduleJob_TeamStandings_JobId" value="Motorsport→Stats→FixedScheduleJob→TeamStandings" />
    <add key="MotorsportFixedScheduleJob_TeamStandings_JobCronExpression" value="30 2 * * *" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricRaceEventsGrids_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricRaceEventsGrid" />
    <add key="MotorsportFixedScheduleJob_HistoricRaceEventsGrids_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricRaces_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricRaces" />
    <add key="MotorsportFixedScheduleJob_HistoricRaces_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricRaceEvents_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricRaceEvents" />
    <add key="MotorsportFixedScheduleJob_HistoricRaceEvents_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricRaceEventsResults_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricRaceEventsResults" />
    <add key="MotorsportFixedScheduleJob_HistoricRaceEventsResults_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricTeamStandings_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricTeamStandings" />
    <add key="MotorsportFixedScheduleJob_HistoricTeamStandings_JobCronExpression" value="0 0 29 2/12000 WED" />
    <!-- Run manually -->
    <add key="MotorsportFixedScheduleJob_HistoricDriverStandings_JobId" value="Motorsport→Stats→FixedScheduleJob→OnceOffHistoricDriverStandings" />
    <add key="MotorsportFixedScheduleJob_HistoricDriverStandings_JobCronExpression" value="0 0 29 2/12000 WED" />
    <add key="Motorsport_LiveRaceJob_JobIdPrefix" value="Motorsport→Stats→LiveJob→" />
    <add key="Motorsport_LiveRaceJob_JobCronExpression" value="*/1 * * * *" />
    <!-- Hangfire dashboard user credentials-->
    <add key="SchedulerDashboardUsername" value="devuser1" />
    <add key="SchedulerDashboardPassword" value="password" />
    <add key="RugbyIngestEnabled" value="true" />
    <add key="MotorsportIngestEnabled" value="true" />
    <add key="maximumTimeForRequestWithResponseInMilliseconds" value="100000" />
    <add key="maximumTimeForResponseInMilliseconds" value="180000" />
    <add key="loggingTtlInMinutes" value="15" />
    <add key="WarningThresholdForJobsTakingTooLongInMinutes" value="30" />
    <add key="ErrorThresholdForJobsTakingTooLongInMinutes" value="60" />
    <add key="MaxNumberOfRecentFixturesToConsider" value="10" />
    <add key="MaxNumberOfHoursToCheckForResults" value="5" />
    <add key="MaxNumberOfHoursToCheckForUpcomingFixtures" value="3" />
    <add key="MaxNumberOfHoursToCheckForPreviousFixtures" value="3" />
    <add key="HalfTimeMaximumDurationInMinutes" value="10" />
    <add key="ManagerLoopTimeInSeconds" value="60" />
    <add key="NumberOfMinutesToCheckForInProgressFixtures" value="120" />
  </appSettings>
