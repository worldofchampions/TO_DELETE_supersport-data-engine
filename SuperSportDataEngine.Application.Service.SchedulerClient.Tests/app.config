<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.InteropServices.RuntimeInformation" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.1.0" newVersion="4.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging.Core" publicKeyToken="af08829b84f0328e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.3.1.0" newVersion="3.3.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <!-- Settings for reference data -->
    <add key="FixedScheduledJob_ReferenceData_JobId" value="FixedScheduleJob→ReferenceData" />
    <!-- Everyday at 2:00am -->
    <add key="FixedScheduledJob_ReferenceData_JobCronExpression" value="0 2 * * *" />
    <!-- Settings for fixtures -->
    <add key="FixedScheduledJob_FixturesData_JobId" value="FixedScheduleJob→Fixtures" />
    <!-- Everyday at 2:05am -->
    <add key="FixedScheduledJob_FixturesData_JobCronExpression" value="5 2 * * *" />
    <!-- Settings for logs -->
    <add key="FixedScheduledJob_LogsData_ActiveTournaments_JobId" value="FixedScheduleJob→Logs→ActiveTournaments" />
    <!-- Everyday at 2:05am -->
    <add key="FixedScheduledJob_LogsData_ActiveTournaments_JobCronExpression" value="5 2 * * *" />
    <!-- Settings for logs -->
    <add key="FixedScheduledJob_LogsData_CurrentTournaments_JobId" value="FixedScheduleJob→Logs→CurrentTournaments" />
    <!-- Every 1 hour -->
    <add key="FixedScheduledJob_LogsData_CurrentTournaments_JobCronExpression" value="0 */1 * * *" />
    <!-- Everyday at 2:05am for a week's data-->
    <add key="FixedScheduledJob_ResultsData_Daily_JobId" value="FixedScheduleJob→Results→AllFixtures" />
    <add key="FixedScheduledJob_ResultsData_Daily_PollingExpression" value="5 2 * * *" />
    <!-- Every hour for the next 3 days-->
    <add key="FixedScheduledJob_ResultsData_Hourly_JobId" value="FixedScheduleJob→Results→EndedFixtures" />
    <add key="FixedScheduledJob_ResultsData_Hourly_PollingExpression" value="0 */1 * * *" />
    <!-- Every 15 minutes on fixture day-->
    <add key="FixedScheduledJob_ResultsData_OnFixtureDay_JobId" value="FixedScheduleJob→Results→CurrentDayFixtures" />
    <add key="FixedScheduledJob_ResultsData_OnFixtureDay_PollingExpression" value="*/15 * * * *" />

    <add key="LiveManagerJob_LiveMatch_JobIdPrefix" value="LiveManagerJob→LiveMatch→" />
    <add key="LiveManagerJob_LiveMatch_JobCronExpression" value="0 */2 * * *" />

    <!-- Settings for logs manager-->
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobIdPrefix" value="ScheduleManagerJob→Logs→" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobCronExpression_15Minutes" value="*/15 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_JobCronExpression_OneMinute" value="*/1 * * * *" />

    <add key="ScheduleManagerJob_Logs_CurrentTournaments_HighFrequencyPolling_CronExpression" value="*/1 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_HighFrequencyPolling_ExpiryInMinutes" value="15" />

    <add key="ScheduleManagerJob_Logs_CurrentTournaments_LowFrequencyPolling_CronExpression" value="*/15 * * * *" />
    <add key="ScheduleManagerJob_Logs_CurrentTournaments_LowFrequencyPolling_ExpiryInMinutes" value="60" />
    
  </appSettings>
