<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of 
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator 
    finds an attribute "name" that has a value of "MyDB".
    
    <connectionStrings>
      <add name="MyDB" 
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True" 
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->
  <appSettings>
    <add key="SuperSportFeed.IpAddress" value="feed.supersport.com"
         xdt:Transform="Replace"
         xdt:Locator="Match(key)" />
  </appSettings>
  <connectionStrings>
    <add name="Redis"
     connectionString="10.10.54.92:7000,10.10.54.93:7000,10.10.54.94:7000,10.10.54.95:7000,10.10.54.96:7000,10.10.54.97:7000,ssl=false,password=JatikIrg7,connectTimeout=10000"
     xdt:Transform="Replace"
     xdt:Locator="Match(name)" />
    <add name="SqlDatabase_PublicSportData"
         connectionString="Data Source=10.10.4.40; Initial Catalog=SuperSportDataEngine_PublicSportData;User ID=SuperSportDataEngine;Password=NSKHXp8JdMfyDX2J"
         providerName="System.Data.SqlClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)" />
    <add name="SqlDatabase_SystemSportData"
         connectionString="Data Source=10.10.4.40; Initial Catalog=SuperSportDataEngine_SystemSportData;User ID=SuperSportDataEngine;Password=NSKHXp8JdMfyDX2J"
         providerName="System.Data.SqlClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)" />
  </connectionStrings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets>
      <target
        xsi:type="Slack"
        name="slackTarget"
        layout="${message}"
        channel="#supersport-data-ops-p"
        username="NLog.Slack"
        compact="false"
        icon=":ghost:"
        webHookUrl="https://hooks.slack.com/services/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"
        xdt:Transform="Replace"
        xdt:Locator="Match(name)"/>
    </targets>
  </nlog>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <!--
      In the example below, the "Replace" transform will replace the entire 
      <customErrors> section of your web.config file.
      Note that because there is only one customErrors section under the 
      <system.web> node, there is no need to use the "xdt:Locator" attribute.
      
      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
</configuration>